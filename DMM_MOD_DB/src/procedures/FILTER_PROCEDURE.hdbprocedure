PROCEDURE "FILTER_PROCEDURE" ( OUT OUT_FILTER VARCHAR(512) )
LANGUAGE SQLSCRIPT
SQL SECURITY DEFINER
READS SQL DATA AS
v_Filter VARCHAR(512);
CURSOR v_Cursor FOR SELECT FILTER FROM "FILTER_TABLE" WHERE USER = SESSION_USER;
BEGIN
OPEN v_Cursor;
FETCH v_Cursor INTO v_Filter;
OUT_FILTER := v_Filter;
CLOSE v_Cursor;
END;
